
<% visitor_events = [] %>
<% @signups.each do |signup| %>
  <%  if signup.visitor_id == @current_visitor.id %>
        <% visitor_events << signup %>
      <%= link_to signup.event.title, signup_path(signup) %> <br>
      <% end %>
    <% end %>

      <% if visitor_events.count ==0 %>
        <h2 style="color:white;">  <%= "You Have Not Currently Signed Up For Any Events" %> </h2>
          
    <% end %>
